{
  "name": "rnwebviewboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:dev": "ENVFILE=.env.development react-native run-android --variant=developmentDebug --appId=com.webviewapp.dev",
    "android:production": "ENVFILE=.env.production react-native run-android --variant=productionRelease --appId=com.webviewapp.prod",
    "android:build:dev": "cd android && ENVFILE=.env.development ./gradlew assembleDevelopmentRelease && cd ..",
    "android:build:production": "cd android && ENVFILE=.env.production ./gradlew assembleProductionRelease && cd .."
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.2",
    "axios": "^0.27.2",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-bootsplash": "3.2.6",
    "react-native-config": "^1.4.6",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-onesignal": "^4.4.1",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.16.0",
    "react-native-webview": "^11.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
